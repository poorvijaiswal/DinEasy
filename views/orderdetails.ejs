<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/orders">Orders</a> / Details</li>
  </ol>
</nav>

<% ro.forEach(function(order) { %>
  <div class="col-6">
    <h4>Table Number: <%= order.table_no %></h4>
    <h4>Total Price: <%= order.total_price %></h4>
  </div>
<% }); %>

<table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Item Name</th>
      <th scope="col">Quantity</th>
      <th scope="col">Total</th>
    </tr>
  </thead>
  <tbody>
    <% rows.forEach(function(item) { %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.quantity %></td>
        <td><%= item.price %></td>
      </tr>
    <% }); %>
  </tbody>
</table>
